    <script>
        window.config = {
            ref: 'app',
            pageName: 'act_ssb_share',
            platform: 'unknown',
            isLogin: 'false' == 'true',
            reportUrl: '/wrpage/report',
            pageLink: '/wrpage/',
            shareIcon: 'https://res.mail.qq.com/node/wr/wrpage/style/images/independent/shareIcon/default$8a1b90b1.png'
        };
        (function (root) {

            var fnList = [];

            var isReady = false;

            root.ready = function (requires, fn)
            {
                if (requires == null)
                {
                    isReady = true;
                    for (var i = 0, len = fnList.length; i < len; i++)
                    {
                        _.use(fnList[i].requires, fnList[i].fn);
                    }
                    fnList = [];

                    return;
                }

                if (fn == null)
                {
                    fn = requires;
                    requires = [];
                }

                if (isReady) return _.use(requires, fn);

                fnList.push({
                    fn: fn,
                    requires: requires
                });
            };

        })(window);
    </script>
